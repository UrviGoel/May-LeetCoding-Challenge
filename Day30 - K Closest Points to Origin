class Solution {
public:
    
    static bool myCompare(vector<int> &p1,vector<int> &p2){
        return (pow(p1[0],2)+pow(p1[1],2)) < (pow(p2[0],2)+pow(p2[1],2));
    }
    
    vector<vector<int>> kClosest(vector<vector<int>>& points, int K) {
        
        sort(points.begin(),points.end(),myCompare);
        points.resize(K);
        return points;
        
    }
};
